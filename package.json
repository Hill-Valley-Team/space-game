  "version": "1.0.0",
  "description": "Project developed during course Middle Frontend Developer at Yandex Practicum",
  "scripts": {
    "build:dev:vendors": "cross-env NODE_ENV=development webpack --config webpack/vendors.config.ts",
    "build:dev:server": "webpack --mode=development --config webpack/server.config.ts",
    "start:dev:webpack": "webpack --mode=development --watch",
    "start:dev:server": "cross-env NODE_ENV=development node index.js --watch dist/server.js",
    "dev": "cross-env NODE_ENV=development run-p start:dev:webpack start:dev:server",
    "prebuild": "cross-env NODE_ENV=production webpack --config webpack/vendors.config.ts",
    "build": "cross-env NODE_ENV=production webpack",
    "start": "cross-env NODE_ENV=production node index.js",
    "lint:p": "prettier --write src/**/*.{js,jsx,ts,tsx,test.tsx,test.ts,json}",
    "lint:ts": "eslint src/**/*.{ts,tsx}",
    "lint:style": "stylelint src/**/*.css",
    "lint": "run-s lint:p lint:ts lint:style",
    "test": "jest",
    "precommit:hook": "npm run lint",
    "init": "npm install && npm run build:dev:vendors && npm run build:dev:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hill-Valley-Team/space-game.git"
